{
  "name": "eslint-config-fluxuator",
  "version": "0.0.0-development",
  "description": "ESLint configuration",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluxuator/eslint-config-fluxuator.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config"
  ],
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "commit": "git-cz",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx,.mdx",
    "lint": "pnpm eslint .",
    "prettier": "prettier --ignore-unknown",
    "format:check": "pnpm prettier --check .",
    "format:fix": "pnpm prettier --write .",
    "prepare": "husky",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "confusing-browser-globals": "^1.0.11",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "commitizen": "^4.3.0",
    "commitlint": "^19.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-testing-library": "^6.2.2",
    "git-cz": "^4.9.0",
    "husky": "^9.0.11",
    "inquirer": "^9.3.5",
    "lint-staged": "^15.2.7",
    "pinst": "^3.0.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "pnpm": {
    "overrides": {
      "normalize-package-data@2.5.0>semver": "^5.7.2",
      "remark-parse@8.0.3>trim": "^0.0.3"
    }
  },
  "peerDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-react": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0 || ^7.0.0",
    "@typescript-eslint/parser": "^6.0.0 || ^7.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.5.0 || ^9.0.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mdx": "^1.16.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-vitest": ">=0.3.0",
    "prettier": "^3.0.0",
    "typescript": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "@babel/eslint-parser": {
      "optional": true
    },
    "@babel/preset-react": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-plugin-mdx": {
      "optional": true
    },
    "eslint-config-prettier": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-jest-formatting": {
      "optional": true
    },
    "eslint-plugin-vitest": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-prettier": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-testing-library": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "tagFormat": "${version}",
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "minor"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "chore",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "packageManager": "pnpm@9.1.4"
}
